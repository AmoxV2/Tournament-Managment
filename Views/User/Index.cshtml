@using Microsoft.AspNetCore.Identity
@model List<AppUser>
﻿@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
    Layout = "SidebarLayout";
}

<div class="main-title">
    <h2>Users</h2>
</div>
<div class="container">
    <div class="row">
        @foreach (var item in Model)
        {

           

            <div class="col-md-4">
                    <div class="card mb-4 box-shadow" style="background-color: rgba(0, 0, 0, 0.1);">
                        <div class="card-body">
                            <img class="card-img-top card-img-top margin-top" src="@item.ImageUrl" alt="Card image cap">
                            <p class="card-title text-light">@item.UserName</p>
                        
                        <p class="card-text text-light">@item.Email</p>
                            <div class="d-flex justify-content-between align-items-center">
                                

                            </div>
                           
                        </div>
                    </div>
                </div>
        
            }
    </div>
</div>




